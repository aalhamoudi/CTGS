{
    "routes": {
        "application": {
            "root": {
                "path": "/",
                "method": "root"
            }
        },
        "users": {
            "create": {
                "path": "/users/new/:tags",
                "method": "create",
                "alternative": "/users/create/:tags"
            },
            "get": {
                "path": "/users/:id/get",
                "method": "get"
            },
            "update": {
                "path": "/users/:id/update/:tags",
                "method": "update"
            },
            "delete": {
                "path": "/users/:id/delete",
                "method": "delete"
            },
            "login": {
                "path": "/users/login/:tags",
                "method": "login"
            }
        }
    }
}